{
    "package": {
        "name": "firamath-travis",
        "repo": "firamath",
        "website_url": "https://firamath.github.io/",
        "issue_tracker_url": "https://github.com/firamath/firamath/issues",
        "vcs_url": "https://github.com/firamath/firamath.git",
        "github_use_tag_release_notes": true,
        "licenses": ["OFL-1.1"],
        "public_download_numbers": true,
        "public_stats": true
    },
    "version": {
        "name": "ciXXXX",
        "desc": "This is a CI build",
        "released": "releaseXXXX",
        "gpgSign": false
    },
    "files": [
        {
            "includePattern": "./src/(FiraMath-*.sfd)",
            "uploadPattern": "$1",
            "matrixParams": {
                "override": 1
            }
        }
    ],
    "publish": true
}