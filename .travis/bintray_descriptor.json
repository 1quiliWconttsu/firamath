{
    "package": {
        "name": "firamath-travis",
        "repo": "firamath",
        "subject": "firamath",
        "desc": "Sans-serif font with Unicode math support",
        "website_url": "https://firamath.github.io/",
        "issue_tracker_url": "https://github.com/firamath/firamath/issues",
        "vcs_url": "https://github.com/firamath/firamath.git",
        "licenses": [
            "MIT"
        ],
        "labels": [
            "font",
            "unicode-math",
            "fira"
        ],
        "github_use_tag_release_notes": true,
        "public_download_numbers": true,
        "public_stats": true
    },
    "version": {
        "name": "<CI-XXXX>",
        "desc": "This is a ci build for firamath",
        "released": "<RELEASE-XXXX>",
        "gpgSign": false
    },
    "files": [
        {
            "includePattern": "release/(firamath-otf-.*\\.zip)",
            "uploadPattern": "$1",
            "matrixParams": {
                "override": 1
            }
        }
    ],
    "publish": true
}